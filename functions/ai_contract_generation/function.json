{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "ai/generate-contract"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 3,
    "delayInterval": "00:00:02"
  }
}